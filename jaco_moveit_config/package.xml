<?xml version="1.0"?>
<package format="3">
  <name>jaco_moveit_config</name>
  <version>0.0.25</version>
  <description>
     An automatically generated package with all the configuration and launch files for using the jaco with the MoveIt Motion Planning Framework
  </description>
  <author email="alex@thoriumrobotics.com">Alex Henning</author>
  <author email="dekent@gatech.edu">David Kent</author>
  <maintainer email="alex@thoriumrobotics.com">Alex Henning</maintainer>
  <maintainer email="dekent@gatech.edu">David Kent</maintainer>
  <maintainer email="rctoris@wpi.edu">Russell Toris</maintainer>

  <license>BSD</license>

  <url type="website">http://ros.org/wiki/jaco_moveit_config</url>
  <url type="bugtracker">https://github.com/RIVeR-Lab/wpi_jaco/issues</url>
  <url type="repository">https://github.com/RIVeR-Lab/wpi_jaco</url>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>moveit_ros_move_group</depend>
  <depend>moveit_planners_ompl</depend>
  <depend>joint_state_publisher</depend>
  <depend>robot_state_publisher</depend>
  <depend>xacro</depend>
  <depend>jaco_description</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
